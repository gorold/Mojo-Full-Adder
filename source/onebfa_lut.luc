module onebfa_lut (
    input in [4],
    output out[2]
  ) {

  always {  
      case (in) {
        4b000: out = 2b00;
        4b001: out = 2b01;
        4b010: out = 2b01;
        4b100: out = 2b01;
        4b011: out = 2b10;
        4b101: out = 2b10;
        4b110: out = 2b10;
        4b111: out = 2b11; 
        default: out = 2b00;
      }
    
  }
}
